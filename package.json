{
  "name": "callmemobiles",
  "version": "1.0.0",
  "description": "CallMeMobiles - Complete mobile repair business management system",
  "author": "CallMeMobiles Team",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:pwa": "npm run generate-icons && tsc && vite build && npm run copy-pwa-assets",
    "generate-icons": "node scripts/generate-pwa-icons.cjs",
    "copy-pwa-assets": "npm run copy-manifest && npm run copy-offline && npm run copy-icons && npm run copy-sw",
    "copy-manifest": "cpx \"public/manifest.json\" dist/",
    "copy-offline": "cpx \"public/offline.html\" dist/",
    "copy-icons": "cpx \"public/icons/**/*\" dist/icons/",
    "copy-sw": "cpx \"public/sw.js\" dist/ || exit 0",
    "preview": "vite preview",
    "preview:pwa": "npm run build:pwa && vite preview --port 4173",
    "electron-dev": "electron .",
    "electron-build": "npm run build && electron-builder",
    "build:android": "powershell -ExecutionPolicy Bypass -File \"build-react-native.ps1\"",
    "build:android-capacitor": "powershell -ExecutionPolicy Bypass -File \"build-all.ps1\" -Android",
    "build:windows": "powershell -ExecutionPolicy Bypass -File \"build-all.ps1\" -Windows",
    "build:all": "powershell -ExecutionPolicy Bypass -File \"build-all.ps1\" -All",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "electron": "electron .",
    "electron-pack": "npm run build && electron-builder --dir",
    "format.fix": "prettier --write .",
    "typecheck": "tsc",
    "build:quick": "powershell -ExecutionPolicy Bypass -File \"quick-build.ps1\" -All",
    "setup:check": "powershell -ExecutionPolicy Bypass -File \"setup-build-environment.ps1\"",
    "setup:java": "powershell -ExecutionPolicy Bypass -File \"setup-build-environment.ps1\" -InstallJava",
    "setup:android": "powershell -ExecutionPolicy Bypass -File \"setup-build-environment.ps1\" -InstallAndroidStudio",
    "cap:sync": "npx cap sync android",
    "cap:open": "npx cap open android",
    "cap:run": "npx cap run android",
    "server": "node server.mjs",
    "server:dev": "node --watch server.mjs",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.0",
    "@capacitor/cli": "^7.4.0",
    "@capacitor/core": "^7.4.0",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@react-three/drei": "^10.1.2",
    "@react-three/fiber": "^8.18.0",
    "@swc/core": "^1.11.24",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-table": "^8.21.3",
    "@types/three": "^0.176.0",
    "bcryptjs": "^2.4.3",
    "caniuse-lite": "^1.0.30001726",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dexie": "^3.2.4",
    "embla-carousel-react": "^8.3.0",
    "express": "^4.18.2",
    "flutter": "^0.0.5",
    "framer-motion": "^12.6.2",
    "input-otp": "^1.2.4",
    "jsonwebtoken": "^9.0.2",
    "lowdb": "^6.1.1",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-native-svg": "^7.0.3",
    "react-resizable-panels": "^2.1.3",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.12.7",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.176.0",
    "uuid": "^9.0.1",
    "vaul": "^0.9.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "electron": "^37.1.0",
    "electron-builder": "^26.0.12",
    "gh-pages": "^6.3.0",
    "globals": "^15.9.0",
    "postcss": "^8.4.47",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.5.3",
    "vite": "^6.2.2",
    "vitest": "^3.1.4"
  },
  "main": "electron-main.cjs",
  "homepage": "https://ritheshh-cmyk.github.io/expenso",
  "build": {
    "appId": "com.callmemobiles.app",
    "productName": "CallMeMobiles",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron-main.cjs",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/favicon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
